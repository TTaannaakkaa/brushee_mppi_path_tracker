<?xml version="1.0"?>
<launch>
  <node pkg="brushee_mppi_path_tracker" type="brushee_mppi_path_tracker_node" name="brsuhee_mppi_path_tracker" output="screen">
    <param name="hz" value="4"/>
    <param name="vx_max" value="0.3"/>
    <param name="vx_min" value="-0.0"/>
    <param name="vx_ref" value="0.2"/>
    <param name="horizon" value="20"/>
    <param name="resolution" value="0.05"/>
    <param name="num_samples" value="1000"/>
    <param name="vel_weight" value="1.5"/>
    <param name="path_weight" value="1.0"/>
    <param name="angle_weight" value="1.5"/>
  </node> 
</launch>